<!doctype html>
<html lang="en" dir="ltr">
    <head>
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css" integrity="sha512-+4zCK9k+qNFUR5X+cKL9EIR+ZOhtIloNl9GIKS57V1MyNsYpYcUrUeQc9vNfzsWfV28IaLL3i96P9sdNyeRssA==" crossorigin="anonymous" />
        <%- include('../common/_head');-%>
    </head>
    
</html> 


<body class="app sidebar-mini" style="font-family: inter-regular;">

    <!-- GLOBAL-LOADER -->
    <div id="global-loader">
        <img src="/images/svgs/loader.svg" class="loader-img" alt="Loader">
    </div>

    <div class="page">
        <div class="page-main">
            <!-- HEADER -->
           
            <%- include('../common/_header');-%>
            <!-- HEADER END -->

            <!-- APP SIDEBAR -->

            <%- include('../common/_leftsidebar');-%>
            
            <!-- APP SIDEBAR CLOSED -->

            <!-- CONTAINER -->
            <div class="app-content">
                <div class="section">

                   

                    <!-- PAGE-HEADER -->
                    <div class="page-header">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="#"><i class="fe fe-home mr-1"></i> Dashboard</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Show Allitem </li>
                        </ol>
                        
                    </div>
                    <!-- PAGE-HEADER END -->

                   
                   
                   
                    <!-- ROW-5 -->
                    <div class="row">
                        <div class="col-12 col-sm-12">
                            <div class="card ">
                                <div class="card-header">
                                    <div class="card-title mb-0">Item Summary</div>
                                </div>
                                <div class="card-body">
                                    <div class="table-responsive">
                                        <% if(items!=null) { %>
                                        <table class="table table-bordered text-nowrap mb-0">
                                            <thead>
                                                <tr>
                                                    <th>Id</th>
                                                    <th>Product Name</th>
                                                    <th>Item Images</th>
                                                    <th>Mrp</th>
                                                    <th>SellingPrice</th>
                                                    <th>Currentstock </th>
                                                    <th>Filter</th>
                                                    <th>Filter Value</th>
                                                    <th>Delevery Term *</th>
                                                    
                                                <th>Action</th> 
                                                    
                                                    
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% items.forEach(function(dat) { %>
                                                <tr>
                                                    
                                                    <td><%= dat._id %></td>
                                                   <td><%= dat.itemname%></td>
                                                   <td>
                                                    <% if(dat.itemImages[0]
                                                    ){ %>
                                                    <img src="<%= dat.itemImages[0]%>" alt="" height="40px"></td><% } %>

                                                    
                                                    
                                                </tr>  
                                               
                                                <% }) %>
                                                </tbody>
                                        </table>
                                        <% } %>      
           
                                    </div>
                                </div>
                            </div>
                        </div><!-- COL END -->
                    </div><!-- ROW-5 END -->
                </div>
                <!-- CONTAINER END -->
            </div>
        </div>

    

        <!-- FOOTER -->
        <%- include('../common/_footer');-%>
        <!-- FOOTER CLOSED -->
    </div>

    <!-- BACK-TO-TOP -->

   
    
   

</body>


