		<!-- BACKGROUND-IMAGE CLOSED -->

		
		<script>
		function myfun(){
			var a=document.getElementById("username").value;
		
			if (a==""){
				document.getElementById("message").innerHTML="** Please fill username";
				return false;
			}
			else{
				document.getElementById("message").innerHTML="";
				
			}

			
		
		var b=document.getElementById("email").value;
		
			if (b==""){
				document.getElementById("messages").innerHTML="** Please fill valid email";
				return false;
			}
			else{
				document.getElementById("messages").innerHTML="";
				
			}
			if (b.indexOf('@')<= 0){
				document.getElementById("messages").innerHTML="** @ Invalid position";
				return false;
			}
			else{
				document.getElementById("messages").innerHTML="";
				
			}

			
		
		var c=document.getElementById("Phoneno").value;

		if(isNaN(c)){
				document.getElementById("messagess").innerHTML="** Please fill numeric phoneNo";
				return false;
			}
			else{
				document.getElementById("messagess").innerHTML="";
				
			}

			if (c==""){

				document.getElementById("messagess").innerHTML="** Please fill valid phoneNo";
				return false;
			}
			else{
				document.getElementById("messagess").innerHTML="";
				
			}


			if(c.length<10){
				document.getElementById("messagess").innerHTML="**Phone no must be 10 digit";
				return false;
			
			}
			else{
				document.getElementById("messagess").innerHTML="";
				
			}

			if(c.length>10){
				document.getElementById("messagess").innerHTML="**Phone no must be 10 digit";
				return false;
			}
			else{
				document.getElementById("messagess").innerHTML="";
				
			}

			if((c.charAt(0)!=9) && (c.charAt(0)!=8) && (c.charAt(0)!=7) && (c.charAt(0)!=6)){
				document.getElementById("messagess").innerHTML="**Phone no must start with 9,8,7,6";
				return false;
			}
			else{
				document.getElementById("messagess").innerHTML="";
				
			}



			var d=document.getElementById("password").value;
		
			if (d==""){
				document.getElementById("messagesss").innerHTML="** Please fill valid password";
				return false;
			}
			else{
				document.getElementById("messagesss").innerHTML="";
				
			}

			if(d.length<6){
				document.getElementById("messagesss").innerHTML="**Password must be 6 digit";
				return false;
			}
			else{
				document.getElementById("messagesss").innerHTML="";
				
			}


			if(d.length>6){
				document.getElementById("messagesss").innerHTML="**Password must be 6 digit";
				return false;
			}
			else{
				document.getElementById("messagesss").innerHTML="";
				
			}


			var e=document.getElementById("confirmpassword").value;
		
			
			if (e==""){
				document.getElementById("messagessss").innerHTML="** Please fill confirmpassword";
				return false;
			}
			else{
				document.getElementById("messagessss").innerHTML="";
				
			}

			if (e!=d){
				document.getElementById("messagessss").innerHTML="** password and confirmpassword must be same";
				return false;
			}
			else{
				document.getElementById("messagessss").innerHTML="";
				
			}


			
		}
			</script>



