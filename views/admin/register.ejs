	<div class="app">

		<!-- BACKGROUND-IMAGE -->
		<div class="login-img">

			<!-- GLOABAL LOADER -->
			<div id="global-loader">
				<img src="../../assets/images/svgs/loader.svg" class="loader-img" alt="Loader">
			</div>

			<div class="page h-100">
				<div class="logo">
				    <!-- CONTAINER OPEN -->
					<div class="col col-login mx-auto">
						<div class="text-center">
							<img src="../../assets/images/lummang.png" width="130px" height="67px" class="header-brand-img" id="img" alt="">
						</div>
					</div>
					<% if(error !=''){%>
						<div class="alert alert-warning alert-dismissible fade show" role="alert" id="error">
							<strong>Sorry!</strong> <%= error %>
							<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
						  </div>
						  <% } %>
					<div class="container-login100">
						<div class="wrap-login100 p-6">
							<form method="post" action="/register"   onsubmit="return myfun()" class="login100-form validate-form"  autocomplete="off">

									 
								<span class="login100-form-title">
									Registration

					
								</span>

								
								<div class="wrap-input100 validate-input" >
									<input class="input100" type="text" name="username" value="" id="username" placeholder="User Name">
									
									<span class="focus-input100"></span>
									<span class="symbol-input100">
										<i class="mdi mdi-account" aria-hidden="true"></i>
									</span>
									<span id="message" style="color: red;"></span> 
								</div>
								<div class="wrap-input100 validate-input" data-validate = "Valid email is required: ex@abc.xyz">
									<input class="input100" type="email" name="email" placeholder="Email" id="email"
									value=""
>
									
									<span class="focus-input100"></span>
									<span class="symbol-input100">
										<i class="zmdi zmdi-email" aria-hidden="true"></i>
									</span>
									<span id="messages" style="color: red;"></span> 
									
								</div>

								<div class="wrap-input100 validate-input" data-validate = "Phoneno is required">
									<input class="input100" type="Phoneno" name="Phoneno" placeholder="Phone No" value="" id="Phoneno"> 
									<span class="focus-input100"></span>
									<span class="symbol-input100">
										<i class="zmdi zmdi-phone" aria-hidden="true"></i>
									</span>
									<span id="messagess" style="color: red;"></span> 
								</div>

								<div class="wrap-input100 validate-input" data-validate = "Password is required">
									<input class="input100" type="password" name="password" placeholder="Password"
									id="password" value="">
									<span class="focus-input100"></span>
									<span class="symbol-input100">
										<i class="zmdi zmdi-lock" aria-hidden="true"></i>
									</span>
									<span id="messagesss" style="color: red;"></span> 
								</div>
								<div class="wrap-input100 validate-input" data-validate = "Password is required">
									<input class="input100" type="password" name="confirmpassword" placeholder="Confirm Password" value="" id="confirmpassword">
									<span class="focus-input100"></span>
									<span class="symbol-input100">
										<i class="zmdi zmdi-lock" aria-hidden="true"></i>
									</span>
									<span id="messagessss" style="color: red;"></span> 
								</div>
								<label class="custom-control custom-checkbox mt-4">
									<input type="checkbox" class="custom-control-input">
									<span class="custom-control-label">Agree the <a href="terms.html">terms and policy</a></span>
								</label>
								<div class="container-login100-form-btn">
