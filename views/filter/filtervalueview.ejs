<!doctype html>
<html lang="en" dir="ltr">
    <head>
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css" integrity="sha512-+4zCK9k+qNFUR5X+cKL9EIR+ZOhtIloNl9GIKS57V1MyNsYpYcUrUeQc9vNfzsWfV28IaLL3i96P9sdNyeRssA==" crossorigin="anonymous" />
        <%- include('../common/_head');-%>
    </head>
    
</html> 


<body class="app sidebar-mini" style="font-family: inter-regular;">

    <!-- GLOBAL-LOADER -->
    <div id="global-loader">
        <img src="/images/svgs/loader.svg" class="loader-img" alt="Loader">
    </div>

    <div class="page">
        <div class="page-main">
            <!-- HEADER -->
           
            <%- include('../common/_header');-%>
            <!-- HEADER END -->

            <!-- APP SIDEBAR -->

            <%- include('../common/_leftsidebar');-%>
            
            <!-- APP SIDEBAR CLOSED -->

            <!-- CONTAINER -->
            <div class="app-content">
                <div class="section">

                   

                    <!-- PAGE-HEADER -->
                    <div class="page-header">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="#"><i class="fe fe-home mr-1"></i> Dashboard</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Show Filter Value </li>
                        </ol>
                        <div class="ml-auto">
                            <a href="/createfiltervalue" class="btn btn-primary btn-icon btn-sm text-white mr-2">
                                <span>
                                    <i class="fe fe-plus"></i>
                                </span> Add Filter Value
                            </a>
                           
                        </div>
                    </div>
                    <!-- PAGE-HEADER END -->

                   
                   
                   
                    <!-- ROW-5 -->
                    <div class="row">
                        <div class="col-12 col-sm-12">
                            <div class="card ">
                                <div class="card-header">
                                    <div class="card-title mb-0">Filter Value Summary</div>
                                </div>
                                <div class="card-body">
                                    <div class="table-responsive">
                                        <% if(filtervalues!=null) { %>
                                        <table class="table table-bordered text-nowrap mb-0">
                                            <thead>
                                                <tr>
                                                    <th>Id</th>
                                                    <th>Filter value</th>
                                                    <th>filter</th>
                                                
                                                    
                                                    <th colspan="2">Action</th>
                                                    
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% filtervalues.forEach(function(dat) { %>
                                                <tr>
                                                    <td><%= dat._id%></td>
                                                    <td><%= dat.filtervalue%></td>
                                                    <td><%= dat.filter_id.filterName%></td>
                                                    
                                                    
                                                    <td>
                                                    <a class="confirm-delete badge badge-danger" href='/deletefiltervalue/<%= dat._id%>' type="submit" class="btn"><i class="fas fa-trash">Delete</i></a>
                    
                                                   
                                                        
                                                        <a class="badge badge-success"  href='/editfiltervalue/<%= dat._id%>' type="submit" class="btn"><i class="fas fa-edit">Edit</i></a>
                                                    </td>
                    
                                                </tr>
                                                <% }) %>
                                                  
                                                </tbody>
                                        </table>
                                        
            <% } %>
                                    </div>
                                </div>
                            </div>
                        </div><!-- COL END -->
                    </div><!-- ROW-5 END -->
                </div>
                <!-- CONTAINER END -->
            </div>
        </div>

    

        <!-- FOOTER -->
        <%- include('../common/_footer');-%>
        <!-- FOOTER CLOSED -->
    </div>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>

    <script>
        $('.confirm-delete').on('click',function(e){
            e.preventDefault();
            var self=$(this)

       

            swal({
  title: "Are you sure?",
  text: "Once deleted, you will not be able to recover this imaginary file!",
  icon: "warning",
  buttons: true,
  dangerMode: true,
})
.then((willDelete) => {
    
    if (willDelete) {
      location.href=self.attr('href');
      swal("Poof! Your imaginary file has been deleted!", {
        icon: "success",
       
      });
      
    } 
  
  
  else {
    swal("Your imaginary file is safe!");
  }
});
        
        }  ) 
    </script>

    <!-- BACK-TO-TOP -->
   

</body>


<!-- GLOBAL-LOADER -->
		
					<!-- APP SIDEBAR CLOSED -->
                   
                    <!-- CONTAINER -->
                    
             
            