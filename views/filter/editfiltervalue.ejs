<!doctype html>
<html lang="en" dir="ltr">
    <head>

        <%- include('../common/_head');-%>
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jstree/3.2.1/themes/default/style.min.css" />

    </head>

</html> 


<body class="app sidebar-mini" style="font-family: inter-regular;">

    <!-- GLOBAL-LOADER -->
    <div id="global-loader">
        <img src="/images/svgs/loader.svg" class="loader-img" alt="Loader">
    </div>

    <div class="page">
        <div class="page-main">
            <!-- HEADER -->
           
            <%- include('../common/_header');-%>
            <!-- HEADER END -->

            <!-- APP SIDEBAR -->

            <%- include('../common/_leftsidebar');-%>
            
            <!-- APP SIDEBAR CLOSED -->

            <!-- CONTAINER -->
            <div class="app-content">
                <div class="section">

                   

                    <!-- PAGE-HEADER -->
                    <div class="page-header">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="#"><i class="fe fe-home mr-1"></i> Masters</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Edit Filter Value</li>
                        </ol>
                        <div class="ml-auto">
                            <a href="/filterview" class="btn btn-primary btn-icon btn-sm text-white mr-2">
                                <span>
                                    <i class="fe fe-plus"></i>
                                </span> Filter List
                            </a>
                        </div>
                    </div>
                    <!-- PAGE-HEADER END -->

                   
                   
                   
                    <!-- ROW-5 -->
                    <div class="row">
                        <div class="col-12 col-sm-12">
                            <div class="card ">
                                <div class="card-header">
                                    <div class="card-title mb-0">Create Filter</div>
                                </div>
                                <div class="card-body">
                                    <div class="table-responsive">
										<form method="post" action="/updatefiltervalue/<%= filtervalues._id %>"
										>
                                        <div class="form-group">
                                            <label for="Category">Filter Value</label>
                                            <input type="text" class="form-control" name="filtervalue" value="<%= filtervalues.filtervalue %>" required  placeholder="Filter Name">
                                            
                                        </div>
 
                                          <div class="form-group">
                                            <label><h4>Filter</h4></label>
                                            
                                            <select class="form-control"   name="filter_id" required  >
                                                
                                            <option><%= filtervalues.filter_id.filterName %> </option>
                                            <% filter.forEach(function(dat) { %>
                                            <option value="<%= dat._id %>"> <%= dat.filterName%> </option>
                                            <% }) %>
                                            </select>
                                           
                                           
                                        </div>
                                        
                                          

                                          
                                        <input class="input100"  name="filtervallueid" placeholder="filtervallueid"
                                        id="filtervallueid" type="hidden" value="<%= filtervalues._id %>" >

                                          
                                            
                                            
                                            <button type="submit" class="btn btn-primary">Save</button>
                                            
                                        </div>	</form>
                                    </div>
                                </div>
                            </div>
                        </div><!-- COL END -->
                    </div><!-- ROW-5 END -->
                </div>
                <!-- CONTAINER END -->
            </div>
        </div>

    

        <!-- FOOTER -->
        <%- include('../common/_footer');-%>
        <!-- FOOTER CLOSED -->
    </div>

    <!-- BACK-TO-TOP -->
   
<script src="https://cdnjs.cloudflare.com/ajax/libs/jstree/3.2.1/jstree.min.js"></script>


</body>


