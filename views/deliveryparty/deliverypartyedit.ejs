<!doctype html>
<html lang="en" dir="ltr">
    <head>

        <%- include('../common/_head');-%>
		
    </head>

</html> 


<body class="app sidebar-mini" style="font-family: inter-regular;">

    <!-- GLOBAL-LOADER -->
    <div id="global-loader">
        <img src="/images/svgs/loader.svg" class="loader-img" alt="Loader">
    </div>

    <div class="page">
        <div class="page-main">
            <!-- HEADER -->
           
            <%- include('../common/_header');-%>
            <!-- HEADER END -->

            <!-- APP SIDEBAR -->

            <%- include('../common/_leftsidebar');-%>
            
            <!-- APP SIDEBAR CLOSED -->

            <!-- CONTAINER -->
            <div class="app-content">
                <div class="section">

                   

                    <!-- PAGE-HEADER -->
                    <div class="page-header">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><i class="fe fe-home mr-1"></i> Masters</li>
                            <li class="breadcrumb-item active" aria-current="page">Edit Deliveryparty</li>
                        </ol>
                        <div class="ml-auto">
                            <a href="/deliverypartyview" class="btn btn-primary btn-icon btn-sm text-white mr-2">
                                <span>
                                    <i class="fe fe-plus"></i>
                                </span> Deliveryparty List
                            </a>
                        </div>
                    </div>
                    <!-- PAGE-HEADER END -->

                   
                   
                   
                    <!-- ROW-5 -->
                    <div class="row">
                        <div class="col-12 col-sm-12">
                            <div class="card ">
                                <div class="card-header">
                                    <div class="card-title mb-0">Update Deliveryparty</div>
                                </div>
                                <div class="card-body">
                                    <div class="table-responsive">
										<form method="post",  action="/postdeliverypartyview" >
										
                                    
                                          
                                          <div class="form-group">
                                            <label for="inputAlias"><h5>Name</h5></label>
                                            <input type="text" class="form-control" id="inputAlias" name="name" value="<%=deliveryparty.name %>" placeholder="name">
                                          </div>
                                          <div class="form-group">
                                            <label for="inputAlias"><h5>Phoneno</h5></label>
                                            <input type="text" class="form-control" id="inputAlias" name="phoneno" value="<%=deliveryparty.phoneno %>" placeholder="phoneno">
                                          </div>
                                          
                                          
                                          <div class="form-group">
                                            
                                            <input type="hidden" class="form-control" id="inputAlias" name="dpId" value="<%=deliveryparty._id %>">
                                          </div>
                                          
                                            
                                            
                                            <button type="submit" class="btn btn-primary">Save</button>
                                        </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div><!-- COL END -->
                    </div><!-- ROW-5 END -->
                </div>
                <!-- CONTAINER END -->
            </div>
        </div>

    

        <!-- FOOTER -->
        <%- include('../common/_footer');-%>
        <!-- FOOTER CLOSED -->
    </div>

    <!-- BACK-TO-TOP -->
   


</body>


